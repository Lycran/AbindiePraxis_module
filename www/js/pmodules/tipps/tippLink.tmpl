<li>
	<% if (typeof tipplink.link == 'string') { %>
		<a href="<%= tipplink.link %>"> <%= tipplink.name %> </a>
	<% } else { %>
		<% if ($.os.ios) { %>
			<a href="<%= tipplink.link.ios %>"> <%= tipplink.name %> </a>
		<% } else if($.os.android) { %>
			<a href="<%= tipplink.link.android %>"> <%= tipplink.name %> </a>
		<% } %>
	<% } %>
</li>

<% %>